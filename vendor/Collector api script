import requests
from requests.auth import HTTPBasicAuth

# Dummy credentials and endpoint
base_url = "https://api.grafana-example.com/fleet"
username = "myuser"
password = "mypassword"
collector_id = "abc123"

def get_config():
    url = f"{base_url}/GetConfig"
    payload = {"collector_id": collector_id}
    response = requests.get(url, json=payload, auth=HTTPBasicAuth(username, password))
    print("GetConfig:", response.status_code)
    print(response.json())

def get_collector():
    url = f"{base_url}/GetCollector"
    payload = {"id": collector_id}
    response = requests.get(url, json=payload, auth=HTTPBasicAuth(username, password))
    print("GetCollector:", response.status_code)
    print(response.json())

def list_collectors():
    url = f"{base_url}/ListCollectors"
    response = requests.get(url, json={}, auth=HTTPBasicAuth(username, password))
    print("ListCollectors:", response.status_code)
    print(response.json())

# Run
get_config()
get_collector()
list_collectors()
